{{#each ac in actionsList}}
		{{#if ac.private}}
			{{#if session.isAuthenticated}}
				<a {{action ac.action }} class="btn btn-xs {{unbound ac.class}} btn-round"> <i class="fa {{unbound ac.icon}}"></i>{{ac.name}}</a>			 
			{{/if}}
		{{else}}
			<a {{action ac.action }} class="btn btn-xs {{unbound ac.class}} btn-round"><i class="fa {{unbound ac.icon}}"></i>{{ac.name}}</a>
		{{/if}}
{{/each}}


